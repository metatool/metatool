{
	"Services": {
		"Input": {
			"Keyboard": {
				// defines alias name of the key to be used inside config files
				// it makes replacement of key used in key combination easier, i.e. to config the GK into Caps
				"KeyAliases": {
					// global key
					"GK": "Space",
					// context key
					"CK": "Enter|Caps",
					// apps key
					"AK": "Apps|Tab"
				}
			}
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	}
}